{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "Update equipment",
    "type": "object",
    "properties": {
      "name": {
        "type": "string",
        "minLength": 1,
        "maxLength": 100
      },
      "status": {
        "type": "string",
        "enum": ["Up", "Down", "Limited"]
      }
    },
    "required": [
      "name",
      "status"
    ],
    "additionalProperties": false
}